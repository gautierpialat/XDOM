<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
    	<add key="nuget.org" value="https://api.nuget.org/v3/index.json" /> 
        <add key="github" value="https://nuget.pkg.github.com/MaxRev-Dev/index.json" />
    </packageSources>
    <packageSourceCredentials>
        <github>
            <add key="Username" value="%GITHUB_API_USER%" />
            <add key="ClearTextPassword" value="%GITHUB_API_KEY%" />
        </github> 
        <nuget.org>
            <add key="Username" value="%NUGET_API_USER%" />
            <add key="ClearTextPassword" value="%NUGET_API_KEY%" />
        </nuget.org>
    </packageSourceCredentials> 
</configuration>
